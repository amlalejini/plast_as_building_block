set walltime 4
set beacon_priority y
set config_dir /mnt/home/lalejini/Data/plast_as_building_block/iter_3/exp_configs
set dest_dir /mnt/home/lalejini/Data/plast_as_building_block/iter_3/exp_data

## Experimental environment runs.
# How does existing regulation of simpler traits affect the evolution of more complex traits?
1..50 Q2T1__rep ./avida -c avida___sensors_NAND_NOT.cfg -s $seed -set EVENT_FILE /mnt/home/lalejini/Data/plast_as_building_block/iter_3/event_bank/events___Q2T1__rep_$seed.cfg -set ENVIRONMENT_FILE environment___ntasks_8__tasks_LOGIC8.cfg -set BIRTH_METHOD 4 -set DIVIDE_SLIP_PROB 0.025 -set SLIP_FILL_MODE 0 -set MIN_GENOME_SIZE 100 -set COPY_MUT_PROB 0.0025
1..50 Q2T2__rep ./avida -c avida___sensors_NONE.cfg -s $seed -set EVENT_FILE /mnt/home/lalejini/Data/plast_as_building_block/iter_3/event_bank/events___Q2T2__rep_$seed.cfg -set ENVIRONMENT_FILE environment___ntasks_8__tasks_LOGIC8.cfg -set BIRTH_METHOD 4 -set DIVIDE_SLIP_PROB 0.025 -set SLIP_FILL_MODE 0 -set MIN_GENOME_SIZE 100 -set COPY_MUT_PROB 0.0025
1..50 Q2T3__rep ./avida -c avida___sensors_NONE.cfg -s $seed -set EVENT_FILE /mnt/home/lalejini/Data/plast_as_building_block/iter_3/event_bank/events___Q2T3__rep_$seed.cfg -set ENVIRONMENT_FILE environment___ntasks_8__tasks_LOGIC8.cfg -set BIRTH_METHOD 4 -set DIVIDE_SLIP_PROB 0.025 -set SLIP_FILL_MODE 0 -set MIN_GENOME_SIZE 100 -set COPY_MUT_PROB 0.0025

# How does existing regulation of simpler traits affect the evolution of the regulation of correlated, more complex traits?
